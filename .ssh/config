Compression yes
ServerAliveInterval 60
ServerAliveCountMax 10000

Host *.oneill.net
  ForwardAgent yes

Host remote.github.net
  User claytono
  ForwardAgent yes
  IdentityFile ~/.ssh/github.id_rsa
  ProxyCommand sh -c "osascript -e 'tell application \"yubiswitch\" to KeyOn'; exec nc %h %p"
  ControlPath ~/.ssh/controlmasters/%r@%h:%p
  ControlMaster auto
  ControlPersist 60m


Host *.github.net
